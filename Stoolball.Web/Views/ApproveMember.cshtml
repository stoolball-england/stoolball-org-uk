@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.ApproveMember>
@using ContentModels = Umbraco.Web.PublishedModels;

<h1>@Model.Name</h1>

@if (Model.ApprovalTokenValid)
{
    @Html.Raw(Model.ApprovalSuccessful.ToHtmlString().Replace("{{NAME}}", Model.MemberName))
}
else
{
    @Model.ApprovalFailed
}