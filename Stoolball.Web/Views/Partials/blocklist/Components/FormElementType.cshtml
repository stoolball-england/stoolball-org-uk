@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<BlockListItem>
@using Stoolball.Web.Models
@using Umbraco.Cms.Core.Models.Blocks;
@{
    var content = (FormElementType)Model.Content;
}
@if (content != null && content.Form != null)
{
    @await Umbraco.RenderMacroAsync("renderUmbracoForm", new { FormGuid = content.Form, FormTheme = "stoolball", ExcludeScripts = "1" })
}