@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<BlockListItem>
@using Umbraco.Cms.Core.Models.Blocks;
@{
    var content = (GettyImage)Model.Content;
}
@if (content != null && content.GettyImageUrl != null)
{
    @:<figure class="block__getty">
        <iframe src="@content.GettyImageUrl" width="594" height="459" frameborder="0" scrolling="no"></iframe>
        <figcaption>@content.Caption</figcaption>
    @:</figure>
}