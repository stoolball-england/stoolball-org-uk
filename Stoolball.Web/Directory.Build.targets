<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Target Name="TransformStoolballWebConfig" BeforeTargets="AfterBuild">
    <Message Text="Running TransformStoolballWebConfig target" />
    <ItemGroup>
      <OriginalWebConfig Include="$(ProjectDir)\Web.config" />
      <TempWebConfig Include="$(ProjectDir)\Web.temp.config" />
      <TempWebConfig2 Include="$(ProjectDir)\Web.temp2.config" />
    </ItemGroup>
    <Copy SourceFiles="@(OriginalWebConfig)" DestinationFiles="@(TempWebConfig)" />
    <TransformXml Source="@(TempWebConfig)" Transform="$(ProjectDir)\Web.local.xdt.config" Destination="@(TempWebConfig2)" StackTrace="true" />
    <TransformXml Source="@(TempWebConfig2)" Transform="$(ProjectDir)\Secret-DataMigrationApiKey.Web.local.xdt.config" Destination="@(OriginalWebConfig)" StackTrace="true" />
    <Delete Files="@(TempWebConfig);@(TempWebConfig2)" />
  </Target>
</Project>